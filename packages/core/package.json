{
  "name": "@awwwkshay/node-ts-core",
  "version": "0.0.1",
  "description": "Core shared utilities",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "oxfmt .",
    "format:check": "oxfmt . --check",
    
    "lint": "oxlint . --fix --type-aware --type-check",

    "clean": "rimraf dist",

    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "zod": "^4.1.13"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}